#include <stdio.h>

int main() 
{ 
    int i; 
    int inicio; 
    int fim; 
    printf("digite o inicio\n"); 
    scanf("%d", &inicio); 
    printf("digite o fim\n");
    scanf("%d", &fim);
    i=inicio;
    
    while (i <=fim){
        if(i%2==0){
        printf("%d\n",i);
        }
    i=i+1;
    }
    return 0;
}
